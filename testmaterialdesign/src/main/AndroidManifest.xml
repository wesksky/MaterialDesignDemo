<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.sky.testmaterialdesign">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".snackerbar.SnackerBarActivity">
        </activity>
        <activity android:name=".textinputlayout.TextInputLayoutActivity">
        </activity>
        <activity android:name=".tablayout.TabLayoutActivity">
        </activity>
        <activity android:name=".collapsingtoolbar.CollapsingToolbarLayoutActivity">
        </activity>
        <activity android:name=".transitions.TransitionsActivity">
        </activity>
        <activity android:name=".transitions.Transition2Activity">
        </activity>
    </application>

</manifest>